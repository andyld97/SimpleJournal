<Window x:Class="SimpleJournal.Dialogs.ExportDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleJournal.Dialogs"
        xmlns:res="clr-namespace:SimpleJournal.Properties" xmlns:fluent="urn:fluent-ribbon" xmlns:controls="clr-namespace:SimpleJournal.Controls"
        mc:Ignorable="d"
        Title="{x:Static res:Resources.strExportPages}" Height="500"  Width="400"
        ResizeMode="NoResize"
        FontSize="14"
        MinWidth="400"
        WindowStartupLocation="CenterScreen" WindowState="Maximized">
    <Grid>
        <controls:ExportControl x:Name="exportControl" />
    </Grid>
</Window>