<Window x:Class="SimpleJournal.AboutDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:res="clr-namespace:SimpleJournal.Properties" xmlns:wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
        xmlns:common="clr-namespace:SimpleJournal.Common;assembly=SimpleJournal.Common" xmlns:modules="clr-namespace:SimpleJournal.Modules"
        mc:Ignorable="d"
        Title="{x:Static res:Resources.strAbout}" Height="670" Width="420"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">

    <modules:AboutModule x:Name="AboutModule" />
</Window>