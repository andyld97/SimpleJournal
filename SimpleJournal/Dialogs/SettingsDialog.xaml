<Window x:Class="SimpleJournal.SettingsDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:modules="clr-namespace:SimpleJournal.Modules"
        xmlns:res="clr-namespace:SimpleJournal.Properties"
        mc:Ignorable="d"
        Title="{x:Static res:Resources.strSettings}"
        Height="420" Width="650"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize">
    
    <modules:SettingsModule x:Name="SettingsModule" />
</Window>